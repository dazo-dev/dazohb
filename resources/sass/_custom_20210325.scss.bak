.partner-section{
	height: auto;
	background: url(#{$assetPath}/slider3.jpg) rgba(0 0 0 / 80%);
    background-size: cover;
    background-blend-mode: multiply;
    padding: 5% 5%;

    .inner-block{
		height: 458px;
		width: 967px;
		background-color: #111111;
		position: relative;
		margin-top: 1em;
		margin-left: 10em;

		.inner-left{
			height: 458px;
			padding: 7% 7%;

			.inner-inner-head{
				font-size: 16px;
				font-weight: 700;
				color: #0D6499;
			}

			.inner-inner-sub{
				font-size: 40px;
				font-weight: bold;
				font-style: italic;
				color: #fff;
			}

			.inner-inner-content{
				font-size: 15px;
				color: #D0D0D0;
			}

			.inner-inner-button{
				padding: 10% 0%;
					button{
						width: 230px;
				    	background-color: #E95720;
				    	font-size: 17px;
				    	color: #fff;
				    	font-weight: 700;
				    	height: 55px;
				    	border-color: #E95720;
					}
			}
		}

		.inner-right{
			height: 458px;

			img{
				margin-top: 15em;
			}
		}

		.floating-horse{
			position: absolute;
			top: -35%;
			margin-top: 0% !important;
			width: 320%;
			left: -50%;
		}
    }

    .partner-header{
    	padding: 0% 40% 0% 10%;
    	margin-top: 1em;

    	.h-sect1{
    		font-size: 18px;
    		color: #fff;
    	}

    	.h-sect2{
    		border: solid 1px #fff;
    		height: 0px;
    		margin-top: 1em;
    	}
    }

    .partners-logo{
    	padding: 0% 40% 0% 10%;
    	margin-top: 1em;

    	.cont-1{
    		h5{
    			font-size: 24px;
    			font-weight: bold;
    			font-style: italic;
    			color: #fff;
    		}
    		p{
				font-size: 16px;
				color: #fff;
    		}
    	}

    	.cont-2{
    		h5{
    			font-size: 24px;
    			font-weight: bold;
    			font-style: italic;
    			color: #fff;
    		}
    		p{
				font-size: 16px;
				color: #fff;
    		}
    	}
    }
