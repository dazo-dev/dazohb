.pointed-block {
    background: #1F1F1F;
    width: 93%;
    position:relative;
       
  }
  .pointed-block:after {
    content: '';
    position: absolute;
    top: 100%;
    left: 0; 
    right: 0;
    padding-bottom: 10%;
    background: #1F1F1F;
    clip-path: polygon(0% 0%, 100% 0%, 50% 100%);
  }


 .display-flex {
