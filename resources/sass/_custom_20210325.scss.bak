				}
			}
			span {
				float: left;
				margin: 8px 0 16px 0px;
				width: 100%;
				height: 40px;
				h1 {
					clear: both;
					float: left;
					margin: 0;
					padding: 0;
					line-height: 1;
					text-align: center;
					width: 100%;
					font-size: 24px;
					line-height: 55px;
				}
				h2 {
					clear: both;
					float: left;
					margin: 0;
					padding: 0;
					line-height: 1;
					text-align: center;
					width: 100%;
					font-size: 14px;
				}
			}
		}
	}

	.ci-cal-container {
		top: 64px;
		position: absolute;
		width: 100%;
		height: calc(100% - 64px);
		.ci-head {
			top: 0;
			position: absolute;
			width: 100%;
			height: 40px;
			div {
				line-height: 40px;
				float: left;
				color: #FFFFFF;
				font-size: 12px;
				font-weight: bold;
				text-align: center;
				text-transform: uppercase;
				height: 100%;
				width: 14.28%;
				width: -moz-calc(100%/7);
				width: -webkit-calc(100%/7);
				width: calc(100%/7);
				position: relative;
				&:first-child{
					color: #0D6499;
				}
				&:last-child {
					border-right: 0px;
					color: #0D6499;
				}
			}
		}
		.ci-body {
			top: 40px;
			position: absolute;
			width: 100%;
			height: calc(100% - 40px);
			.ci-row {
				clear: both;
				float: left;
				position: relative;
				
				width: 100%;
				height: 16.6%;
				&:last-child {
					border-bottom: 0;
				}
				.ci-event-row {
					position: absolute;
					left: 0px;
					padding: 3px;
					height: 50%;
					width: 100%;
					z-index: 10;
					&:nth-child(1) {
						top: 0px;
					}
					&:nth-child(2) {
						margin-top: -1px;
						top: 50%;
					}
				}
				>div {
					
					
					overflow: hidden;
					float: left;
					height: 100%;
					width: 14.28%;
					width: -moz-calc(100%/7);
					width: -webkit-calc(100%/7);
					width: calc(100%/7);
					position: relative;
					&:last-child {
						border-right: 0;
					}
					.ci-date {
						top: 12px;
						left: 50%;
						position: absolute;
						font-size: 12px;
					}
				}
				>div.empty {
					/*background: #f5f5f5;*/
					&:before {
						background: url(http://www.transparenttextures.com/patterns/black-twill.png);
						position: absolute;
						height: 100%;
						width: 100%;
						opacity: 0.125;
					}
				}
				>div.ci-today {
					background: #000;
					color: #E95720;

					/*&:after{
						content: '';
						width: 0;
						height: 0;
						border-style: solid;
						border-width: 0 10px 10px 0;
						border-color: transparent #E95720 transparent transparent;
						right: 0;
						top: 0;
						position: absolute;
					}*/
				}
			}
		}
		.ci-calendar.cal-rows-four {
			.ci-body {
				.ci-row {
					width: 100%;
					height: 25%;
				}
			}
		}
		.ci-calendar.cal-rows-five {
			.ci-body {
				.ci-row {
					width: 100%;
					height: 20%;
				}
			}
		}
		.ci-calendar.cal-rows-six {
			.ci-body {
				.ci-row {
					width: 100%;
					height: 16.666%;
				}
			}
		}
	}

	.ci-event {
		border-radius: 3px;
		border: 1px solid transparent;
		position: absolute;
		padding: 2px 12px;
		height: calc(100% - 5px);
		z-index: 20;
		label {
			font-size: 12px;
			font-weight: bold;
			vertical-align: middle;
		}
	}

	.end-cap {
		top: 0.5px;
		z-index: 1;
		position: absolute;
		width: 0px;
		height: 0px;
		border-style: solid;
		border-color: transparent;
	}
	.start-cap {
		top: 0.5px;
		z-index: 1;
		position: absolute;
		width: 0px;
		height: 0px;
		border-style: solid;
		border-color: transparent;
	}
	.end-cap-border {
		top: -1.75px;
		z-index: 0;
		position: absolute;
		width: 0px;
		height: 0px;
		border-style: solid;
		border-color: transparent;
	}
	.start-cap-border {
		top: -1.75px;
		z-index: 0;
		position: absolute;
		width: 0px;
		height: 0px;
		border-style: solid;
		border-color: transparent;
	}

	.ci-event.public {
		.end-cap {
			border-color: transparent #e6f1cd;
		}
		.start-cap {
			border-color: transparent #e6f1cd;
		}
		.end-cap-border {
			border-color: transparent #D1DBBB;
		}
		.start-cap-border {
			border-color: transparent #D1DBBB;
		}
		background: #e6f1cd;
		border-color: #D1DBBB;
	}
	.ci-event.private {
		.end-cap {
			border-color: transparent #cceff2;
		}
		.start-cap {
			border-color: transparent #cceff2;
		}
		.end-cap-border {
			border-color: transparent #9FBABD;
		}
		.start-cap-border {
			border-color: transparent #9FBABD;
		}
		background: #cceff2;
		border-color: #9FBABD;
	}

	@media (max-width:360px) {
		.ci-cal-container {
			.ci-body {
				float: left;
				height: auto;
				position: relative;
				.ci-row {
					border: 0px;
					height: auto !important;
					.ci-event-row {
						top: 0px;
						clear: both;
						float: left;
						height: auto;
						padding: 0px !important;
						margin-top: 0px !important;
						position: relative;
					}
					div {
						display: none;
					}
				}
			}
		}
		.ci-event {
			float: left;
			position: relative;
			min-height: 100px;
			left: 0px !important;
			margin: 5px !important;
			width: calc(100% - 10px) !important;
		}
		.end-cap {
			display: none;
		}
		.end-cap-border {
			display: none;
		}
		.start-cap {
			display: none;
		}
		.start-cap-border {
			display: none;
		}
	}
/*Calendar End*/
