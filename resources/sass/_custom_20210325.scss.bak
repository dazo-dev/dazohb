    /*Calendar Start*/

    	.ci-clickable{
    		cursor: pointer;
    	}

    	header {
			top: 0;
			position: absolute;
			width: 100%;
			height: 64px;
			.title-content {
				position: absolute;
				width: 100%;
				height: 100px;
				text-align: center;
				button {
					position: absolute;
					top: 12px;
					height: 40px;
					text-align: center;
					width: 140px;
					background-color: #0000;
					border: none;
				}
				button#last {
					left: 12px;
					.fas{
					  	color: #0D6499;
					}
				}
				button#next {
					right: 12px;
					.fas{
					  	color: #0D6499;
					}
				}
				span {
					float: left;
					margin: 8px 0 16px 0px;
					width: 100%;
					height: 40px;
					h1 {
						clear: both;
						float: left;
						margin: 0;
						padding: 0;
						line-height: 1;
						text-align: center;
						width: 100%;
						font-size: 24px;
						line-height: 55px;
					}
					h2 {
						clear: both;
						float: left;
						margin: 0;
						padding: 0;
						line-height: 1;
						text-align: center;
						width: 100%;
						font-size: 14px;
					}
				}
			}
		}

		.ci-cal-container {
			top: 64px;
			position: absolute;
			width: 100%;
			height: calc(100% - 64px);
			.ci-head {
				top: 0;
				position: absolute;
				width: 100%;
				height: 40px;
				div {
					line-height: 40px;
					float: left;
					color: #FFFFFF;
					font-size: 12px;
					font-weight: bold;
					text-align: center;
					text-transform: uppercase;
					height: 100%;
					width: 14.28%;
					width: -moz-calc(100%/7);
					width: -webkit-calc(100%/7);
					width: calc(100%/7);
					position: relative;
					&:first-child{
						color: #0D6499;
					}
					&:last-child {
						border-right: 0px;
						color: #0D6499;
					}
				}
			}
			.ci-body {
				top: 40px;
				position: absolute;
				width: 100%;
				height: calc(100% - 40px);
				.ci-row {
					clear: both;
					float: left;
					position: relative;
					
					width: 100%;
					height: 16.6%;
					&:last-child {
						border-bottom: 0;
					}
					.ci-event-row {
						position: absolute;
						left: 0px;
						padding: 3px;
						height: 50%;
						width: 100%;
						z-index: 10;
						&:nth-child(1) {
							top: 0px;
						}
						&:nth-child(2) {
							margin-top: -1px;
							top: 50%;
						}
					}
					>div {
						
						
						overflow: hidden;
						float: left;
						height: 100%;
						width: 14.28%;
						width: -moz-calc(100%/7);
						width: -webkit-calc(100%/7);
						width: calc(100%/7);
						position: relative;
						&:last-child {
							border-right: 0;
						}
						.ci-date {
							top: 12px;
							left: 50%;
							position: absolute;
							font-size: 12px;
						}
					}
					>div.empty {
						/*background: #f5f5f5;*/
						&:before {
							background: url(http://www.transparenttextures.com/patterns/black-twill.png);
							position: absolute;
							height: 100%;
							width: 100%;
							opacity: 0.125;
						}
					}
					>div.ci-today {
						background: #000;
						color: #E95720;

						/*&:after{
							content: '';
							width: 0;
							height: 0;
