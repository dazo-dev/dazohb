.text-spacing {
	font-weight: 700;
    letter-spacing: 3px;
}


.link-area a{
	color: #fff !important;
	font-size: 18px;
	font-weight: bold;
	padding-left: 0rem !important;
}

.btn-area a{
	color: #fff !important;
	background-color: #E95720;
    border-radius: 10px;
    width: 120px;
    text-align: center;
    text-transform: uppercase;
    margin-left: 5px;
    font-size: 18px;
    padding: 15px 0px;
}

.profile a{
	color: #fff !important;
    font-size: 18px;
}

.carousel{
	padding-top: 0%;
}

.carousel-caption{
	right: unset;
    bottom: 5%;
    text-align: left;
    width: 60%;
    left: 10%;
}

.carousel-caption li:first-child{
	background-color: #000;
    transform: skew(-20deg);
    list-style-type: none;
    padding: 20px 40px;
    width: 40%;
}

.carousel-caption li:first-child h5{
	transform: skew(25deg);
	font-size: 22px;
	font-style: italic;
	font-weight: bold;
	margin: 0;
	padding: 0;
}

.carousel-caption p{
	font-size: 55px;
	font-style: italic;
	font-weight: bold;
	line-height: 4rem;
}

