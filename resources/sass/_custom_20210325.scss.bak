.pointe-block-3 {
	width: 100%;
	border-bottom: none;
	position: relative;
	background-color: #1F1F1F;
}
.pointe-block-3:after, .pointe-block-3:before {
  	content: "";
	width: 0;
	height: 0;
	border-style: solid;
	border-width: 1rem 5rem 1rem 5rem;
	border-color: #ffffff00 transparent transparent transparent;
	top: 100%;
	left: -1px;
	position: absolute;
}
.pointe-block-3:after {
  border-color: #1F1F1F transparent transparent transparent;
  top: calc(100% - 1px);
  
}

.pointe-block-4 {
	width: 100%;
	border-bottom: none;
	position: relative;
	background-color: #1F1F1F;
}
.pointe-block-4:after, .pointe-block-4:before {
	content: "";
	width: 0;
