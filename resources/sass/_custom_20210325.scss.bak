    .block-header {
	    font-size: 16px;
	    font-weight: bold;
	    color: #0D6499;
	    margin-left: 5%;
	}

	.block-sub{
		font-size: 40px;
	    font-style: italic;
	    font-weight: bold;
	    color: #fff;
	    margin-left: 5%;
	    width: 170%;
	}

	.block-content{
		font-size: 15px;
		color: #fff;
	    margin-left: 5%;
	    width: 160%;
	}

	.block-btn{
		margin-left: 5%;
