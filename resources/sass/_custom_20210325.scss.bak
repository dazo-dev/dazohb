#map {
	width: 100%;
	height: 405px;
}

.map-section{
	background-color: #000;
	height: auto;
    text-align: center;
    color: #fff;

    .left-block{
    	padding: 2% 5% 0% 10%;

    	.block-layer{
    		height: 50px;
    		width: 100%;
    		margin-bottom: 5px;

    		.header{
    			font-size: 30px;
    			font-weight: bold;
    			font-style: italic;
    			text-transform: uppercase;
    		}

    		select{
				background-color: #707070;
				border: none;
				color: #fff;
				border-radius: 0;
				padding: 8px 20px;
				height: 50px;
    		}

    	}

    	.block-layer-result{
    		height: 20px;
    		width: 100%;
    		margin-bottom: 5px;

    		.result-title{
    			color: #FFFFFF;
    			font-size: 13px;
    			padding: 2% 5%;
    		}

    		.result-items{
				font-size: 14px;
				font-weight: 600;
				color: #FFFFFF;
				padding: 0% 10%;
    		}

    		.active-result{
				color: #E95720 !important;
    		}
    	}

		._mapTargetAddress:hover {
			color: #E95720;
			cursor: pointer;
		}
    }
}

