.accordion-section{
	height: auto;
    background-color: #000;
    padding: 5% 15%;

    .block-header{
    	h1{
			text-align: center;
			width: 100%;
			font-size: 40px;
			font-style: italic;
			font-weight: bold;
			color: #fff;
    	}
    }

    .block-sub{
    	p{
    		padding: 1% 10%;
    		font-size: 17px;
    		color: #fff;
    		text-align: center;
    	}
    }

    .block-accordion{
    	.card{
			border-color: #1A1A1A;
			border-radius: 0px;
			color: #fff;
			margin: 1% 0%;

			.card-header{
				background-color: #1A1A1A;
				padding: 0;

				h2{
					font-size: 14px;
					font-weight: 500;
					text-transform: uppcase;
				}

				.btn-block{
					background: #0D6499;
					padding: 20% 20%;

					i{
						color: #fff;
					}

					button{
						background-color: #0D6499;
						border-radius: 0;
					}
				}

				.title-block{
					font-size: 16px;
					font-weight: 600;
					padding: 2% 2%;
					text-transform: uppercase;
				}
			}


