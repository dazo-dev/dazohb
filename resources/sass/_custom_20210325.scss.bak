/*background and font-colors*/
.bg-black{
	background-color: #000;
}

.content-white{
	color: #fff;
}

.modal-open{overflow-y:auto;overflow-x:hidden;padding-right:0 !important;}

.modal-open .modal{overflow-y: hidden;}

.hidden-btn{ display: none;	}

.invalid-feedback{
	display: block;
    width: 100%;
    margin-top: 5%;
    font-size: 15px;
    color: #e3342f;
    text-align: center;
    margin-bottom: -5%;
}



#app .navbar{
	background-color: #000 !important;
	position: fixed;
    z-index: 9999;
    width: 100%;
    height: 92px !important;
}

.full-width{
	margin-right: 0px;
    margin-left: 0px;
    width: 100%;   
}

.navbar-light .navbar-toggler{
	color: rgb(255 255 255) !important;
	border-color: #E95720 !important;
}

.navbar-toggler{
	background-color: #E95720 !important;
    border: 1px solid #e95720 !important;
}

.nav-login,
.nav-register{
	border-radius: 0px !important;

	&:hover{
		color: #fff;
	}
}

.nav-register{
	background-color: #6B6C6C !important;
}

.pagination{
    display: inline-flex !important;

    .paginate_button{
    	a{
			font-weight: bolder !important;
			color: #fff !important;
			background-color: transparent !important;
			border-color: transparent !important;
    	}
    }
    .active{
    	.page-link{
			color: #E95720 !important;
			background-color: transparent !important;
			border-color: transparent !important;
			text-decoration: none !important;
			position: relative !important;
    	}

    	.page-link:after {
			content: "";
			width: 70%;
			position: absolute;
			left: 15%;
			bottom: 5px;
			border-width: 0 0 1px;
			border-style: solid;
		}
    	
    }
}
div.dataTables_wrapper div.dataTables_paginate{
    text-align: center !important;
}

.table-bordered{
	th{
		border: 1px solid #000000 !important;
	}
	td{
		border: 1px solid #000000 !important;
	}
}

#jockeyTable{
	width:100%; color: #fff;
	border-color: transparent !important;
	thead{
		border-color: transparent !important;
		tr{
			th{
				text-align: center;
				background-color: #000;
			}

			th:first-child{
				text-align: left;
			}
		}
	}
	tbody{
		.even{
		background-color: #181818;
		}
		.odd{
			background-color: #222222;
		}

		tr{
			td:nth-child(2){
				background-color: #001E32;
			}

			td{
				text-align: center;
				color: #c6c6c6;
			}

			td:first-child{
				text-align: left;
			}
		}


	}
	tr:nth-child(odd) td {
		background-color: rgb(29, 28, 28);
	}
	tr:nth-child(even) td {
		background-color: rgb(63, 62, 62);
	}
	
}

.horse-details-section {
	margin-top: 5%;
	background-color: #000;
	.horsename {
		padding-top: 5%;
		font-weight: 700;
		font-style: italic;
	}
	.row-profile-bottom {
		padding-top: 5%;
		color: #c6c6c6;
	}
	.table-horseprofile {
		td:nth-child(2) {
			padding-left: 3%;
		}
		p {
			margin-bottom: 5px;
		}
		td {
			color: #c6c6c6;
		}
	}
	
}




