@font-face {
    font-family:'NexaBold';
    src: url('/fonts/NexaBold.otf') format('otf');
    font-style: normal;
    font-weight: normal;
}

$assetPath :"/dazohb/public/images";

body{
	overflow-x: hidden;
	font-family: 'Roboto';
	padding-right: 0px !important;
}	

.py-4 .container{
	margin-top: 3%;
	padding: 0;
}

div.dataTables_wrapper div.dataTables_filter{
	display: none;
}

.navbar-nav{
	.nav-link{
		font-size: 15px;
		&:hover{
			color: #fff !important;
		}
	}
}

.table-bordered{
	border: 1px solid transparent !important;
}

.multi-column-ul{
	-moz-column-count: 2;
    -moz-column-gap: 20px;
    -webkit-column-count: 4;
    -webkit-column-gap: 20px;
}

.jockey-bio{
	.nav-tabs{
		border: none !important;
		.nav-link{
			color: #fff !important;
			background-color: transparent !important;
			border: none !important;
		}
		.active{
			color: #E95720 !important;
    text-decoration: underline;
    text-underline-position: under;
		}
	}

	.tab-content{
		div{
			padding: 1% 3%;
		}
	}
}

